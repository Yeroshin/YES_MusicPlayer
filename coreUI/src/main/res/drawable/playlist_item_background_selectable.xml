<?xml version="1.0" encoding="utf-8"?>
<layer-list
    xmlns:android="http://schemas.android.com/apk/res/android">


    <item
        android:bottom="2dp">
        <shape
            android:shape="rectangle">
            <solid android:color="@color/light" />

        </shape>
    </item>

    <item
        android:top="2dp">
        <shape
            android:shape="rectangle">
            <solid android:color="@color/black" />

        </shape>
    </item>
    <item
        android:top="2dp"
        android:bottom="2dp">
        <shape
            android:shape="rectangle">
            <solid android:color="@color/branded_color" />

        </shape>
    </item>
    <item
        >
        <shape
            android:shape="rectangle">

            <stroke
                android:width="2dp"
                android:color="@color/item_selectable_state_list"
                android:dashWidth="2dp"
                android:dashGap="2dp" />

        </shape>
    </item>

</layer-list>